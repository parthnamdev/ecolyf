    <title>My Rides</title>
</head>
<body onload="hide_loading()">
  <div class="load_div">
    <div class="spinner-border text-info p-4 spin-load" role="status">
      <div class="sr-only">Loading...</div>
    </div>
  </div>
  
  <section class="main-background">
    <div class="container-fluid">
  <section id="header">
    <div class="">
        <nav class="navbar navbar-expand-lg">
            <a class="navbar-brand" href="/user"><img src="bicycle.png" alt="" style="width: 40px;"> Ecolyf</a>
            <button
              class="navbar-toggler"
              type="button"
              data-bs-toggle="collapse"
              data-bs-target="#navbarSupportedContent"
              aria-controls="navbarSupportedContent"
              aria-expanded="false"
              aria-label="Toggle navigation"
            >
            <i class="fas fa-ellipsis-v nav-toggler-icon"></i>
            <!-- <span class="navbar-toggler-icon"></span> -->
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
              <ul class="navbar-nav ms-auto">
                <li class="nav-item">
                  <a class="nav-link" href="/user">Explore</a>
                </li>
               
                <li class="nav-item">
                  <a class="nav-link" href="/user/rides">Rides</a>
                </li>
                <li class="nav-item">
                  <div class="dropdown">
                    <button class="btn dropdown-toggle nav-btn mx-auto" type="button" id="dropdownMenuButton" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                      Profile <i class="fas fa-user-circle"></i>
                    </button>
                    <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                      <a class="dropdown-item" href="/user/profile">My Account</a>
                      <div class="dropdown-divider"></div>
                      <a class="dropdown-item" href="/logout">Logout <i class="fas fa-sign-out-alt"></i></a>
                    </div>
                  </div>
                </li>
              </ul>
            </div>
          </nav>
    </div>
</section>
<section id="upcoming" class="row top-margin">
  <h1 class="mb-5">Upcoming</h1>
  <% if (upcoming.length > 0) { %>
  <% upcoming.forEach(element => { %>
  <div class="col-lg-10 card transactions-card mb-3 mx-auto">
    <div class="card-body">
    <table class="custom-table">
      <tr><td class="font-weight-bold custom-td first-col">ID</td><td class="custom-td"><%= element.uuid %></td></tr>
      <tr><td class="font-weight-bold custom-td first-col">User ID</td><td class="custom-td"><%= element.userUuid %></td></tr>
      <tr><td class="font-weight-bold custom-td first-col">Cycle UUID</td><td class="custom-td"><%= element.cycleUuid.uuid %></td></tr>
      <tr><td class="font-weight-bold custom-td first-col">Cycle Type</td><td class="custom-td"><%= element.cycleUuid.cycleType %></td></tr>
      <tr><td class="font-weight-bold custom-td first-col">Distance</td><td class="custom-td"> <%= element.distance %></td></tr>
      <tr><td class="font-weight-bold custom-td first-col">Timestamp</td><td class="custom-td"> <%= element.time %></td></tr>
      <tr><td class="font-weight-bold custom-td first-col">From stand</td><td class="custom-td"> <%= element.from %></td></tr>
      <tr><td class="font-weight-bold custom-td first-col">To stand</td><td class="custom-td"> <%= element.to %></td></tr>
    </table>
  </div>
  </div>
  <% }); %>
  <% }else{ %>
    <h5 class="green">No rides</h5>
    <% } %>
</section>
