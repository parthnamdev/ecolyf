      <tr><td class="font-weight-bold custom-td first-col">To stand</td><td class="custom-td"> <%= element.to %></td></tr>
    </table>
  </div>
  </div>
  <% }); %>
  <% }else{ %>
    <h5 class="green">No rides</h5>
    <% } %>
</section>
    
 
  </div>
