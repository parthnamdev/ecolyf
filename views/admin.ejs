  <script>
    function animate(data, id) {
      return bodymovin.loadAnimation({
        container: document.getElementById(id), 
        path: data, 
        renderer: 'svg',
        loop: true, 
        autoplay: true, 
      });
    }

    var img1 = animate('bike0.json', 'img1');
    var img2 = animate('discount.json', 'img2');
    var img3 = animate('bike.json', 'img3');
    var img4 = animate('eco.json', 'img4');
  </script>
