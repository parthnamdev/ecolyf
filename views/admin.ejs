            <div class="col-lg-6 mt-5">
              <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="cycleType" id="Geared" value="Geared" checked>
                <label class="form-check-label" for="Geared">Geared</label>
              </div>
              <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="cycleType" id="Regular" value="Regular">
                <label class="form-check-label" for="Regular">Regular</label>
              </div>
              <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="cycleType" id="Diva" value="Diva">
                <label class="form-check-label" for="Diva">Diva</label>
              </div>
            </div>
            </div>
              <div class="row">
              <button type="submit" class="btn btn-outline-primary btn-seller col-sm-3 mx-auto">Add</button>
              </div>
