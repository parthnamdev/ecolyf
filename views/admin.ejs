    <section id="location" class="">
  
          <form action="/admin/addcycle" method="POST" class="auth-form text-center">
            <div id="stickySearchBar">
      
              <div class="sticky-search-box mx-auto">
                  <!-- <form method="post" action="/user/getAvail" id="wordSubmit" enctype="application/x-www-form-urlencoded"> -->
                    <input method="" name="input" action="" class="sticky-input-box search-tag" type="text" id="word_input" autocomplete="off" autofocus required
                      placeholder="Search location" />
                      
                    <!-- <button class="sticky-button" href="" id="searchbtn"> -->
                      <!-- <i class="fas fa-search"></i> -->
                    <!-- </button> -->
                  <!-- </form> -->
                  <script>
                    var input = document.getElementById("word_input");
                
                      input.addEventListener("keyup", function (event) {
                      // Number 13 is the "Enter" key on the keyboard
                      if (event.keyCode === 13) {
                        // Cancel the default action, if needed
                        event.preventDefault();
                        // Trigger the button element with a click
                        document.getElementById("searchbtn").click();
                      }
                    });
                  </script>
                  <div class="suggestions"></div>
                  
                </div>
              </div>
            <div class="row col-lg-7 col-md-8 col-sm-10 mx-auto">
            <div class="col-lg-6">
